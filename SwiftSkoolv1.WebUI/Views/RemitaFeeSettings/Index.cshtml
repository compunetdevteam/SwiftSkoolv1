@model IEnumerable<SwiftSkoolv1.Domain.RemitaFeeSetting>

@{
    ViewBag.Title = "List Remita Setting";
}
<div class="card">
    <div class="card-block">

        <div class="form-header  purple darken-4">
            <h3>
                List of Remita Setting <span>
                    <a href="@Url.Action("Create","RemitaFeeSettings")" class="btn btn-primary btn-lg"> <i class="fa fa-plus left"></i> Add Remita Setting</a>
                </span>
            </h3>
        </div>

        <table class="table table-responsive">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.School.Name)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ServiceType)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.MerchantId)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ApiKey)
                </th>
                <th></th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.School.Name)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.ServiceType)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.MerchantId)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.ApiKey)
                    </td>
                    <td>
                        @Html.ActionLink("Edit", "Edit", new { id = item.RemitaFeeSettingId }) |
                        @Html.ActionLink("Details", "Details", new { id = item.RemitaFeeSettingId }) |
                        @Html.ActionLink("Delete", "Delete", new { id = item.RemitaFeeSettingId })
                    </td>
                </tr>
            }

        </table>
    </div>
</div>
